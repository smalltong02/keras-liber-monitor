  "NtQueryLicenseValue": {
    "Signature": {
      "Library": "ntdll.dll",
      "Return": "NTSTATUS",
      "Feature": "P65"
    },

    "Hooks": {
      "Category": "^(?i)category_normal$"
    },

    "Parameters": {
      "ValueName": "LPUNICODE_STRING",
      "Type": "LPULONG",
      "Data": "LPVOID",
      "DataSize": "ULONG",
      "ResultDataSize": "LPULONG"
    },

    "Ensure": [
      "ValueName"
    ],

    "PreCheck": {
      "0": {
        "Check": {
          "ValueName->Buffer": "%var0 =~ '^(?i)kernel-vmdetection-private$'"
        },

        "Modify": {
          "Return": "%var1 = 3221225524"
        }
      }
    }
  },

  "GetPwrCapabilities": {
    "Signature": {
      "Library": "PowrProf.dll",
      "Return": "BYTE",
      "Feature": "P66"
    },
    "Hooks": {
      "Category": "^(?i)category_normal$"
    },
    "Parameters": {
      "lpspc": "LPSYSTEM_POWER_CAPABILITIES"
    },
    "Ensure": [
      "lpspc"
    ],
    "PostCheck": {
      "0": {
        "Check": {
          "lpspc->SystemS1": "%var0 == 0"
        },

        "Modify": {
          "lpspc->SystemS1": "%var1 = 1"
        }
      }
    }
  },

