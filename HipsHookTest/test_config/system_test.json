  "ExitWindowsEx": {
    "Signature": {
      "Library": "user32.dll",
      "Return": "BOOL",
      "Feature": "P41"
    },

    "Parameters": {
      "uFlags": "EXITFLAGS",
      "dwReason": "EXITREASON"
    },

    "PreLog": [
      "uFlags",
      "dwReason"
    ],

    "PreCheck": {
      "0": {
        "Define": {
          "uFlags": "DWORD"
        },
        "Check": {
          "uFlags": "(%var0 & 1) || (%var0 & 2) || (%var0 & 8)"
        },
        "Modify": {
          "Return": "%var1 = 1"
        }
      }
    }
  },

