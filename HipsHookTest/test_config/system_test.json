  "EnumProcesses": {
    "Signature": {
      "Library": "Kernel32.dll",
      "Return": "BOOL",
      "Feature": "P67"
    },

    "Hooks": {
      "Category": "^(?i)category_normal$|^(?i)category_test$"
    },

    "Parameters": {
      "lpidProcess": "DWORD*",
      "cb": "DWORD",
      "lpcbNeeded": "LPDWORD"
    },

    "Ensure": [
      "lpidProcess",
      "lpcbNeeded"
    ],

    "PostCheck": {
      "0": {
        "CCode": {
          "main": [
            "if(lpcbNeeded != 'nullptr' && *lpcbNeeded > sizeof(DWORD)) {",
            "DWORD cProcesses = *lpcbNeeded / sizeof(DWORD)",
            "DWORD i = 0",
            "while(i < cProcesses) {",
            "if(lpidProcess[i] != 0) {",
            "LogPrint(i , lpidProcess[i])",
            "}",
            "i = i + 1",
            "}",
            "}"
          ]
        }
      }
    }
  },

