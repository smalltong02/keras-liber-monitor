  "NtQueryInformationProcess": {
    "Signature": {
      "Library": "ntdll.dll",
      "Return": "NTSTATUS",
      "Feature": "P62"
    },

    "Hooks": {
      "Category": "^(?i)category_normal$|^(?i)category_test$"
    },

    "Parameters": {
